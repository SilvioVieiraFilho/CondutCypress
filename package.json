{
  "name": "cypressplaygrounds",
  "scripts": {
    "delete:reports": "rimraf cypress/reports && mkdir cypress\\reports",
    "prereport": "npm run delete:reports",
    "junit:merge": "npx junit-merge -d \"cypress/reports/junit\" -o \"C:/Users/karen/Documents/Projetos/CondutCypress/cypress/reports/junit/results.xml\"",
    "cy:run:all" : "npm run delete:reports && npx cypress run || true && npm run junit:merge"
  },
  "devDependencies": {
    "cypress": "^15.6.0",
    "cypress-iframe": "^1.0.1",
    "cypress-mochawesome-reporter": "^4.0.2",
    "cypress-multi-reporters": "^2.0.5",
    "junit-merge": "^2.0.0",
    "mocha-junit-reporter": "^2.2.1",
    "rimraf": "^5.0.0"
  }
}
